<h2>Danh sách thành viên</h2>
<a href="NvkCreate" class="btn btn-primary" >Them moi</a>
<table class="table table-bordered">
    <tr>
        <th>Mã sinh viên</th>
        <th>Username</th>
        <th>Họ và tên</th>
        <th>Password</th>
        <th>Email</th>
    </tr>
    @foreach (var m in ViewBag.nvkMember)
    {
        <tr>
            <td>@m.NvkMemberId</td>
            <td>@m.NvkUserName</td>
            <td>@m.NvkFullName</td>
            <td>@m.NvkPassword</td>
            <td>@m.NvkEmail</td>
        </tr>
    }
</table>